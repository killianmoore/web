{
  "name": "killian-moore-studio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:pd": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "lint:pd": "eslint app/pd-lab components/pd-*.tsx lib/pd-*.ts --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "typecheck:pd": "tsc -p tsconfig.pd.json --noEmit",
    "verify:pd": "npm run lint:pd && npm run typecheck:pd && npm run build:pd",
    "cap:add:ios": "npx cap add ios",
    "cap:sync": "npx cap sync ios",
    "cap:sync:auto": "node scripts/pd-cap-sync-ios.mjs",
    "cap:open:ios": "npx cap open ios"
  },
  "dependencies": {
    "@capacitor/cli": "^8.1.0",
    "@capacitor/core": "^8.1.0",
    "@capacitor/ios": "^8.1.0",
    "alchemy-sdk": "^3.6.5",
    "framer-motion": "^11.11.17",
    "next": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.35",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
